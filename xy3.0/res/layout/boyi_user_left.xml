        android:orientation="vertical"
        
        >
        <RelativeLayout
        android:id="@+id/shelf_account"
        android:layout_width="wrap_content"
        android:layout_height="53.3dp"
        android:layout_marginLeft="26.6dp"
        
         >

        <ImageView
            android:id="@+id/shelf_account_img"
            android:layout_width="22.7dp"
            android:layout_height="22.7dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5.0dip"
            android:layout_marginRight="5.0dip"
            android:background="@drawable/user_setting" />

        <TextView
            android:id="@+id/shelf_account_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/shelf_account_img"
            android:ellipsize="end"
            android:maxWidth="@dimen/menu_textView_maxWidth"
            android:singleLine="true"
            android:layout_marginRight="5dp"
            android:text="@string/boyi_user_setting"
            android:textColor="#5d5d5d"
            android:layout_marginLeft="11dp"
            android:textSize="16sp"
             />
        <ImageView 
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:background="@drawable/boe_user_go"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="21.6dp"
            />
